{
  "name": "stock-chic-backend",
  "version": "1.0.0",
  "description": "Backend API pour Stock Chic - Gestion de stock de vêtements",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "watch": "ts-node --watch src/server.ts",
    "migrate": "node scripts/migrate-demo-data.js",
    "setup": "npm run migrate"
  },
  "keywords": [
    "stock",
    "vêtements",
    "api",
    "express",
    "typescript"
  ],
  "author": "Stock Chic Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
